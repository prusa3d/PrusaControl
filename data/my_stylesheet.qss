QProgressBar {
    font-family: 'Titillium Web';
    font-size: 12px;
    height: 27px;
    margin: 0px 10px 0px 30px;
    border-radius: 5px;
    background-color: rgb(132, 132, 132);
    color:rgb(255, 255, 255);
    text-align: center;
}

QProgressBar::chunk {
    background-color: rgb(255, 97, 0);
    border-radius: 5px;
}

QProgressBar#splash_progressbar {
    background-color: rgb(255, 255, 255);
}

QSpinBox,
QDoubleSpinBox {
    font-family: 'Titillium Web';
    font-size: 12px;
    height: 22px;
    color: rgb(255, 255, 255);
    border-radius: 5px;
    background-color:rgb(132, 132, 132);
    padding-right: 15px;
}

QSpinBox:disabled,
QDoubleSpinBox:disabled {
    color: rgb(112, 112, 112);
    background-color:rgb(96, 96, 96);
}

QSpinBox::up-button,
QDoubleSpinBox::up-button{
    image: url(base_dirdata/img/gui/arrow_up_objectadjust_off.png);
}

QSpinBox::up-button:hover,
QDoubleSpinBox::up-button:hover{
    image: url(base_dirdata/img/gui/arrow_up_objectadjust_hover.png);
}

QSpinBox::up-button:pressed,
QDoubleSpinBox::up-button:pressed{
    image: url(base_dirdata/img/gui/arrow_up_objectadjust_on.png);
}

QSpinBox::down-button,
QDoubleSpinBox::down-button{
    image: url(base_dirdata/img/gui/arrow_down_objectadjust_off.png);
}

QSpinBox::down-button:hover,
QDoubleSpinBox::down-button:hover{
    image: url(base_dirdata/img/gui/arrow_down_objectadjust_hover.png);
}

QSpinBox::down-button:pressed,
QDoubleSpinBox::down-button:pressed{
    image: url(base_dirdata/img/gui/arrow_down_objectadjust_on.png);
}


QCheckBox {
    spacing: 5px;
    font-size: 12px;
}

QCheckBox::indicator {
    width: 42px;
    height: 17px;
}

QCheckBox::indicator:checked:hover {
    image: url(base_dirdata/img/gui/checkbox_on_hover.png);
}

QCheckBox#brimCheckBox::indicator:unchecked {
    image: url(base_dirdata/img/gui/checkbox_dark_off.png);
}


QCheckBox#brimCheckBox::indicator:unchecked:hover {
    image: url(base_dirdata/img/gui/checkbox_dark_off_hover.png);
}

QCheckBox::indicator:unchecked {
    image: url(base_dirdata/img/gui/checkbox_off.png);
    color: rgb(0,0,0);
}

QCheckBox::indicator:checked {
    image: url(base_dirdata/img/gui/checkbox_on.png);
}

QCheckBox::indicator:checked:disabled {
    image: url(base_dirdata/img/gui/checkbox_on_disabled.png);
    color: rgb(0,0,0);
    background-color: transparent;
}

QCheckBox::indicator:unchecked:disabled {
    image: url(base_dirdata/img/gui/disable_checkbox_off.png);
    color: rgb(0,0,0);
    background-color: transparent;
}

QCheckBox::indicator:unchecked:hover {
    image: url(base_dirdata/img/gui/checkbox_off_hover.png);
}

QCheckBox#brimCheckBox::indicator:unchecked:disabled {
    image: url(base_dirdata/img/gui/checkbox_dark_off_disabled.png);
}


QCheckBox#lock_axis_checkbox{
    spacing: 5px;
    font-size: 12px;
}

QCheckBox#lock_axis_checkbox::indicator {
    width: 42px;
    height: 51px;
}

QCheckBox#lock_axis_checkbox::indicator:checked {
    image: url(base_dirdata/img/gui/scalelock_on.png);
}

QCheckBox#lock_axis_checkbox::indicator:unchecked {
    image: url(base_dirdata/img/gui/scalelock_off.png);
}

QCheckBox#lock_axis_checkbox::indicator:checked:hover {
    image: url(base_dirdata/img/gui/scalelock_on_hover.png);
}

QCheckBox#lock_axis_checkbox::indicator:unchecked:hover {
    image: url(base_dirdata/img/gui/scalelock_off_hover.png);
}

QCheckBox#lock_axis_checkbox::indicator:checked:disabled {
    image: url(base_dirdata/img/gui/scalelock_on_disabled.png);
}

QCheckBox#lock_axis_checkbox::indicator:unchecked:disabled {
    image: url(base_dirdata/img/gui/scalelock_off_disabled.png);
}
