QComboBox {
    margin: 0px 10px 0px 0px;
    padding: 0px 0px 1px 3px;
    font-size: 12px;
    height: 22px;
    background-color:rgb(77, 77, 77);
    border: 1px solid rgb(77, 77, 77);
    border-radius: 5px;
    color:rgb(255, 255, 255);
}


QComboBox:hover,
QPushButton:hover {
    border: 1px solid rgb(255, 255, 255);
    color: rgb(255, 255, 255);
}
/*
QComboBox:on {
}*/

QComboBox::drop-down {
    background-color:rgb(77, 77, 77);
    color:rgb(255, 255, 255);
    border-radius: 5px;
}

QComboBox::drop-down:disabled {
    color: rgb(112, 112, 112);
    background-color:rgb(89, 89, 89);
    border-radius: 5px;
}

QComboBox::down-arrow {
    image: url(base_dirdata/img/gui/arrow_dropdownmenu.png);
    width: 13px;
    height: 13px;
}

QComboBox::down-arrow:disabled {
    image: url(base_dirdata/img/gui/arrow_dropdownmenu.png);
    width: 13px;
    height: 13px;
    color: rgb(112, 112, 112);
    background-color:rgb(89, 89, 89);
}


QComboBox:disabled{
    color: rgb(130, 130, 130);
    background-color:rgb(96, 96, 96);
    border: 1px solid rgb(96, 96, 96);
}

QComboBox::down-arrow:disabled{
    color: rgb(130, 130, 130);
    background-color:rgb(96, 96, 96);
}

QComboBox::drop-down:disabled{
    color: rgb(130, 130, 130);
    background-color:rgb(96, 96, 96);
}

QComboBox#materialCombo QAbstractItemView,
QComboBox#qualityCombo QAbstractItemView,
QComboBox#supportCombo QAbstractItemView,
QComboBox#infillCombo QAbstractItemView,
QComboBox#extruder1_c QAbstractItemView,
QComboBox#extruder2_c QAbstractItemView,
QComboBox#extruder3_c QAbstractItemView,
QComboBox#extruder4_c QAbstractItemView,
SettingsDialog QComboBox QAbstractItemView
{
    margin: 0px 10px 0px 0px;
    padding: 0px 0px 0px 0px;
    background-color:rgb(132, 132, 132);
    selection-background-color: rgb(255, 97, 0);
    selection-color: rgb(25, 25, 25);
    color:rgb(0, 0, 0);
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 5px;
    outline: 0px;
    height: 300px;
}


/*TEST*/
QComboBox#object_extruder_c, QComboBox#combobox_scale_units {
    font-size: 12px;
    height: 22px;
    margin: 0px 0px 0px 0px;
    padding: 0px 0px 1px 3px;
    border: 1px solid rgb(132, 132, 132);
    background-color:rgb(132, 132, 132);
    color:rgb(255, 255, 255);
    border-radius: 5px;
}

QComboBox#object_extruder_c:hover, QComboBox#combobox_scale_units:hover {
    border: 1px solid rgb(255, 255, 255);
    color: rgb(255, 255, 255);
}

QComboBox#object_extruder_c:disabled, QComboBox#combobox_scale_units:disabled {
    color: rgb(112, 112, 112);
    background-color:rgb(96, 96, 96);
    border: 1px solid rgb(96, 96, 96);
}

QComboBox#object_extruder_c:editable, QComboBox#combobox_scale_units:editable {
    background: transparent;
}

QComboBox#object_extruder_c:!editable, QComboBox#object_extruder_c::drop-down:editable,
QComboBox#combobox_scale_units:!editable, QComboBox#combobox_scale_units::drop-down:editable{

}

/* QComboBox gets the "on" state when the popup is open */
QComboBox#object_extruder_c:!editable:on, QComboBox#object_extruder_c::drop-down:editable:on,
QComboBox#combobox_scale_units:!editable:on, QComboBox#combobox_scale_units::drop-down:editable:on{

}

QComboBox#object_extruder_c:on, QComboBox#combobox_scale_units:on { /* shift the text when the popup opens */

}

QComboBox#object_extruder_c::drop-down, QComboBox#combobox_scale_units::drop-down {
    border-radius: 5px;
    background-color:rgb(132, 132, 132);
    color:rgb(255, 255, 255);
}

QComboBox#object_extruder_c::drop-down:disabled, QComboBox#combobox_scale_units::drop-down:disabled {
    color: rgb(112, 112, 112);
    background-color:rgb(96, 96, 96);
    border: 1px solid rgb(96, 96, 96);
    border-radius: 5px;
}

QComboBox#object_extruder_c::down-arrow, QComboBox#combobox_scale_units::down-arrow {
    image: url(base_dirdata/img/gui/arrow_dropdownmenu_bright.png);
    width: 13px;
    height: 13px;
}

QComboBox#object_extruder_c::down-arrow:disabled, QComboBox#combobox_scale_units::down-arrow:disabled  {
    color: rgb(112, 112, 112);
    background-color:rgb(96, 96, 96);
    image: url(base_dirdata/img/gui/arrow_dropdownmenu_bright.png);
    width: 13px;
    height: 13px;
}

QComboBox#object_extruder_c::down-arrow:on { /* shift the arrow when popup is open */

}

QComboBox#object_extruder_c QAbstractItemView, QComboBox#combobox_scale_units QAbstractItemView {
    background-color:rgb(132, 132, 132);
    selection-background-color: rgb(255, 97, 0);
    /*selection-color: rgb(25, 25, 25);*/
    color:rgb(255, 255, 255);
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 5px;
    outline: 0px;
}


